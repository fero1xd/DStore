{
  "name": "DStore",
  "version": "1.0.0",
  "description": "Use discord as a file storage, for educational purposes only",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start:build": "node dist/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "migrations:generate": "npx drizzle-kit generate:pg --schema ./src/database/schema.ts",
    "migrations:push": "ts-node ./src/database/migrate.ts",
    "ci": "pnpm run typecheck && pnpm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "async-mutex": "^0.4.0",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "mime": "^4.0.0",
    "mime-types": "^2.1.35",
    "postgres": "^3.4.3",
    "ts-node-dev": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@neondatabase/serverless": "^0.6.0",
    "@types/mime-types": "^2.1.4",
    "drizzle-kit": "^0.20.6"
  }
}
